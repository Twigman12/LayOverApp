# API Keys
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_DIRECTIONS_API_KEY=your_google_directions_api_key_here

# Backend API
API_BASE_URL=https://api.layover-app.com
API_VERSION=v1

# Environment
NODE_ENV=development
EXPO_PUBLIC_ENVIRONMENT=development

# Feature Flags
ENABLE_REAL_TIME_FLIGHT_TRACKING=false
ENABLE_PUSH_NOTIFICATIONS=false
ENABLE_OFFLINE_MODE=false

# Analytics
ANALYTICS_ENABLED=false
ANALYTICS_KEY=your_analytics_key_here

# Storage
SECURE_STORE_KEY=layover_app_secure_key

# Maps Configuration
DEFAULT_MAP_REGION_LAT=40.7128
DEFAULT_MAP_REGION_LNG=-74.0060
DEFAULT_MAP_REGION_DELTA=0.0922 